        <script>
            $('#register-email-n').submit(function (e) {
                e.preventDefault();
                $('#btn-register-email').attr('disabled', 'disabled');
                var formRegister = $(this);
                formRegister.append('<div class="register-email-return col-12 mt-4"><div class="loader">Carregando...</div></div>');
                $.ajax({
                    method: "POST",
                    url: "https://www.tmg.agr.br/ptbr/newsletter/register",
                    data: formRegister.serialize()
                }).done(function (response) {
                    if (response.status == "success") {
                        var responseTextClass = "background-text-green";
                        $('#newsletter-email').val('');
                    } else {
                        var responseTextClass = "background-text-orange";
                    }
                    $('.register-email-return').html('<h2 class="' + responseTextClass + '">' + response.msg + '</h2>');
                }).fail(function () {
                    $('.register-email-return').html('<h2 class="background-text-orange">Erro ao cadatrar seu e-mail, tente novamente.</h2>');
                }).always(function () {
                    setTimeout(function () {
                        formRegister.find('.register-email-return').fadeOut('slow').remove();
                    }, 3000);
                    $('#btn-register-email').removeAttr('disabled');
                });
            });
        </script>
        
    </body>
